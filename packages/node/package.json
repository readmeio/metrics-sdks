{
  "name": "readmeio",
  "description": "Track your API metrics within ReadMe.",
  "version": "7.0.0-alpha.8",
  "license": "ISC",
  "homepage": "https://readme.com/metrics",
  "bugs": {
    "email": "support@readme.io",
    "url": "https://github.com/readmeio/metrics-sdks/issues"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:readmeio/metrics-sdks.git"
  },
  "keywords": [
    "api-metrics",
    "readme"
  ],
  "engines": {
    "node": ">=18"
  },
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "files": [
    "dist"
  ],
  "dependencies": {
    "content-type": "^1.0.5",
    "find-cache-dir": "^3.3.2",
    "flat-cache": "^6.1.1",
    "lodash": "^4.17.15",
    "ssri": "^12.0.0",
    "timeout-signal": "^1.1.0",
    "type-is": "^1.6.18"
  },
  "devDependencies": {
    "@readme/eslint-config": "^14.0.0",
    "@tsconfig/node18": "^18.2.4",
    "@types/content-type": "^1.1.5",
    "@types/express": "^4.17.21",
    "@types/find-cache-dir": "^3.2.1",
    "@types/flat-cache": "^2.0.0",
    "@types/har-format": "^1.2.15",
    "@types/lodash": "^4.14.197",
    "@types/node": "^20.14.12",
    "@types/ssri": "^7.1.1",
    "@types/supertest": "^2.0.12",
    "@types/type-is": "^1.6.3",
    "@vitest/coverage-v8": "^2.0.4",
    "eslint": "^8.34.0",
    "express": "^4.18.2",
    "is-valid-uuid-v4": "^1.2.3",
    "mock-req": "^0.2.0",
    "msw": "^2.3.4",
    "prettier": "^3.0.2",
    "supertest": "^6.3.3",
    "typescript": "^5.1.6",
    "vitest": "^2.0.4"
  },
  "scripts": {
    "build": "tsc",
    "lint": "npm run lint:types && npm run lint:js",
    "lint:js": "eslint . --ext js,ts",
    "lint:types": "tsc --noEmit",
    "prebuild": "rm -rf dist/",
    "prepack": "npm run build",
    "pretest": "npm run build",
    "prettier": "prettier --list-different --write \"./**/**.{js,ts}\"",
    "test": "vitest run --coverage"
  },
  "prettier": "@readme/eslint-config/prettier",
  "optionalDependencies": {
    "pino": "^9.3.2",
    "pino-pretty": "^13.0.0"
  }
}
