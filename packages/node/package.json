{
  "name": "readmeio",
  "description": "Track your API metrics within ReadMe.",
  "version": "4.2.0",
  "license": "ISC",
  "homepage": "https://readme.com/metrics",
  "bugs": {
    "email": "support@readme.io",
    "url": "https://github.com/readmeio/metrics-sdks/issues"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:readmeio/metrics-sdks.git"
  },
  "keywords": [
    "api-metrics",
    "readme"
  ],
  "engines": {
    "node": "^12 || ^14 || ^16"
  },
  "dependencies": {
    "content-type": "^1.0.4",
    "find-cache-dir": "^3.3.1",
    "flat-cache": "^3.0.4",
    "lodash": "^4.17.15",
    "node-fetch": "^2.6.0",
    "timeout-signal": "^1.1.0",
    "uuid": "^8.2.0"
  },
  "devDependencies": {
    "@readme/eslint-config": "^6.1.0",
    "body-parser": "^1.19.0",
    "conventional-changelog-cli": "^2.1.1",
    "eslint": "^7.29.0",
    "express": "^4.17.1",
    "is-valid-uuid-v4": "^1.2.0",
    "jest": "^27.0.5",
    "nock": "^13.1.0",
    "prettier": "^2.3.1",
    "rimraf": "^3.0.2",
    "supertest": "^6.0.1"
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "prettier": "prettier --list-different --write \"./**/**.js\"",
    "release": "conventional-changelog -i CHANGELOG.md -s",
    "test": "jest --coverage"
  },
  "jest": {
    "globalSetup": "<rootDir>/__tests__/jest.setup.js",
    "testMatch": [
      "**/__tests__/**/(*.)+(spec|test).[jt]s?(x)",
      "**/?(*.)+(spec|test).[jt]s?(x)"
    ]
  },
  "prettier": "@readme/eslint-config/prettier"
}
